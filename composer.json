{
    "name": "peynman/larapress-pages",
    "description": "PWA and Page rendering and management for Larapress.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "Peyman Abdi",
        "email": "larapress@peynman.ir"
    }],
    "autoload": {
        "psr-4": {
            "Larapress\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larapress\\Pages\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larapress\\Pages\\Providers\\PackageServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^8.0",
        "peynman/larapress-ecommerce": "master"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.2.0",
        "phpunit/phpunit": "^9.0",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "3.*",
        "matthewbdaly/artisan-standalone": "dev-master"
    }
}
